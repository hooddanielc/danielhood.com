{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["es6", "dom", "dom.iterable"],
    "module": "es6",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "outDir": ".webpack",
    "removeComments": true,
    "rootDir": "./src",
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "es6"
  }
}
